<% @lista_posts.each do |p| %>
<div class="card-post">
  <div class="card-image">
    <%= image_tag p.image_url %>
  </div>
  <div class="card-date"><%= p.created_at %></div>
  <div class="card-title">
    <h2><%= p.title %></h2>
  </div>
  <div class="card-content">
  <%= p.content %>
  </div>
</div>
<% end %>
